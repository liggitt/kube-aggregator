apiVersion: v1
clusters:
- cluster:
    insecure-skip-tls-verify: true
    server: https://localhost:6443
  name: local-secure
contexts:
- context:
    cluster: local-secure
    namespace: default
    user: master
  name: local-secure
current-context: local-secure
kind: Config
preferences: {}
users:
- name: master
  user:
    token: master/system:masters